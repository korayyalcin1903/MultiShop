@model UserDetailViewModel
@{
    Layout = null;
}



<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br />
    <h3>Kullanıcı Profil Bilgisi</h3>
    <div class="alert alert-info">Id:@Model.Id</div>
    <div class="alert alert-warning">Username:@Model.Username</div>
    <div class="alert alert-info">Ad Soyad:@Model.Username @Model.Surname</div>
    <div class="alert alert-warning">Email:@Model.Email</div>
</div>
